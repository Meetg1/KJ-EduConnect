 </body>
 <script>
         
        $ (function (){
        $('#searchName').autocomplete({
          source: function(req, res){
            $.ajax({
              url:"/autocomplete/",
              dataType:"jsonp",
              type:"GET",
              data:req,
              success: function(data){
                res(data)
              }, 
              error: function(err){
                console.log(err);
              }
            });
          },
          minlength: 1,
          select: function(event, ui){
            if(ui.item){
              
              var text = ui.item.label;
              var text2 = text.split(",")[0].substring(7);
              
              ui.item.label = text2;
              console.log("time: "+ui.item.label);
              $('#searchName').text("hiiii      "+ui.item.value);
            }
          } 
          
          
        });
});

// $( ".searchName" ).on( "autocompleteselect", function( event, ui ) {
